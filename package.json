{
  "name": "semantic-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^2.0.1",
    "@types/webassembly-js-api": "^0.0.1",
    "immutable": "^3.8.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.3.0",
    "redux": "^3.7.2",
    "slate": "^0.32.4",
    "slate-react": "^0.11.4",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "npm-run-all start:trigger --parallel start:cargo-wasm start:cargo-core-build start:cargo-core-run start:react",
    "start:trigger": "touch .trigger",
    "start:cargo-wasm": "cargo watch -w src -x 'build --release --target=wasm32-unknown-unknown --features lint' -s scripts/run-wasm-gc -s scripts/run-wasm-bindgen -s scripts/run-wasm2es6js",
    "start:cargo-core-build": "cargo watch -w src -x 'check --features lint' -s 'touch .trigger'",
    "start:cargo-core-run": "RUST_BACKTRACE=1 cargo watch --no-gitignore -w .trigger -x 'run -- --debug'",
    "start:react": "react-app-rewired start",
    "build": "npm-run-all build:cargo-wasm build:wasm-gc build:wasm-bindgen build:wasm2es6js build:react build:cargo-core",
    "build:cargo-wasm": "cargo build --release --target=wasm32-unknown-unknown --features lint",
    "build:cargo-core": "cargo build --release --features 'lint standalone'",
    "build:wasm-gc": "scripts/run-wasm-gc",
    "build:wasm-bindgen": "scripts/run-wasm-bindgen",
    "build:wasm2es6js": "scripts/run-wasm2es6js",
    "build:react": "react-app-rewired build",
    "test": "npm-run-all test:cargo test:react",
    "test:cargo": "cargo test",
    "test:react": "react-app-rewired test --env=jsdom"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.3",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-transition-group": "^2.0.7",
    "@types/uuid": "^3.4.3",
    "npm-run-all": "^4.1.2",
    "react-app-rewire-typescript": "^2.0.2",
    "react-app-rewired": "^1.5.0",
    "react-scripts": "^1.0.13",
    "typescript": "^2.6.2",
    "webpack": "^3.11.0"
  }
}
